page {
  background:#ffffff;
}
.van-tabs__line {
  height:2px !important;
  background-color:#0096ff !important;  
}
.myVideo{
  margin-top: 4rpx;
  width:100%;
}
.margin-bottom {
  margin-bottom: 7rpx;
}
.padding{
  padding:16rpx 30rpx;
}
.textStyle{
  color: #a6a6a6;
  font-size: 32rpx;
  text-align: right;
}
.textStyle text{
 margin-left: 10rpx
}
.text{
  color: #3d3d3d;
